    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "faker": "^5.4.0",
    "helmet": "^4.4.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
