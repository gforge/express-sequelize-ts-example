{
  "name": "express-sequelize-ts-example",
  "version": "1.1.0",
  "description": "A basic example of using express with sequelize and typescript.",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && node dist/app.js",
    "dev": "BABEL_DISABLE_CACHE=1 concurrently --kill-others --names \"tsc,node\" -c \"blue,green\" \"tsc --noEmit --pretty --watch\" \"nodemon --exec babel-node --extensions '.ts,.graphql' src/app.ts\"",
    "build": "tsc --noEmit --pretty && babel --extensions '.ts,.tsx' --source-maps --ignore *.spec.ts  --out-dir dist src"
  },
